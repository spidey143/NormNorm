        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NormNorm"
        mc:Ignorable="d"
        Title="Journal" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label>Фильтр</Label>
            <ListBox x:Name="groups" ItemsSource="{Binding Groups}" Height="54"></ListBox>
        </StackPanel>
    </Grid>
</Window>
